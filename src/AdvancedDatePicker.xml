<?xml version="1.0" encoding="utf-8"?>
<widget id="mendix.advanceddatepicker.AdvancedDatePicker" pluginWidget="true"
    needsEntityContext="true"
    offlineCapable="true"
    supportedPlatform="Web"
    xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Advanced Date Picker</name>
    <description>Mendix date picker with most of the customization available in the React Date Picker library</description>
    <helpUrl>https://github.com/bsgriggs/mendix-advanced-date-picker/blob/master/README.md</helpUrl>
    <icon />
    <properties>
        <propertyGroup caption="General">
            <propertyGroup caption="Label">
                <systemProperty key="Label" />
                <property key="placeholder" type="textTemplate" required="false">
                    <caption>Placeholder</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US"></translation>
                    </translations>
                </property>
            </propertyGroup>
            <propertyGroup caption="Format">
                <property key="dateFormat" type="enumeration" defaultValue="DATE">
                    <caption>Date format</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="DATE">Date</enumerationValue>
                        <enumerationValue key="MONTH">Month</enumerationValue>
                        <enumerationValue key="QUARTER">Quarter</enumerationValue>
                        <enumerationValue key="YEAR">Year</enumerationValue>
                        <enumerationValue key="TIME">Time</enumerationValue>
                        <enumerationValue key="DATETIME">Date and Time</enumerationValue>
                        <enumerationValue key="CUSTOM">Custom</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="timeInterval" type="expression" required="true" defaultValue="15">
                    <caption>Time interval</caption>
                    <description></description>
                    <returnType type="Integer" />
                </property>
                <property key="timeCaption" type="textTemplate" required="true">
                    <caption>Time caption</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Time</translation>
                        <translation lang="nl_NL">Tijd</translation>
                    </translations>
                </property>
                <property key="customDateFormat" type="textTemplate" required="true">
                    <caption>Custom date format</caption>
                    <description></description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Data source">
                <property key="selectionType" type="enumeration" defaultValue="SINGLE">
                    <caption>Selection type</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="SINGLE">Single</enumerationValue>
                        <enumerationValue key="RANGE">Range</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="dateAttribute" type="attribute" required="true" onChange="onChange">
                    <caption>Date attribute</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
                <property key="startDateAttribute" type="attribute" required="true">
                    <caption>Start date attribute</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
                <property key="endDateAttribute" type="attribute" required="true"
                    onChange="onChange">
                    <caption>End date attribute</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Selectable Dates">
            <propertyGroup caption="Min and Max">
                <property key="minDate" type="expression" required="false">
                    <caption>Min date</caption>
                    <description></description>
                    <returnType type="DateTime" />
                </property>
                <property key="maxDate" type="expression" required="false">
                    <caption>Max date</caption>
                    <description></description>
                    <returnType type="DateTime" />
                </property>
            </propertyGroup>
            <propertyGroup caption="Specific days">
                <property key="specificDaysMode" type="enumeration" defaultValue="OFF">
                    <caption>Mode</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="OFF">Off</enumerationValue>
                        <enumerationValue key="INCLUDE">Include</enumerationValue>
                        <enumerationValue key="EXCLUDE">Exclude</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="specificDaysDatasource" type="datasource" isList="true"
                    required="true">
                    <caption>Datasource</caption>
                    <description></description>
                </property>
                <property key="specificDaysAttribute" type="attribute"
                    dataSource="specificDaysDatasource" required="true">
                    <caption>Attribute</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
            </propertyGroup>
            <propertyGroup caption="Interval days">
                <property key="intervalDaysMode" type="enumeration" defaultValue="OFF">
                    <caption>Mode</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="OFF">Off</enumerationValue>
                        <enumerationValue key="INCLUDE">Include</enumerationValue>
                        <enumerationValue key="EXCLUDE">Exclude</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="intervalDaysDatasource" type="datasource" isList="true"
                    required="true">
                    <caption>Datasource</caption>
                    <description></description>
                </property>
                <property key="intervalDaysStart" type="attribute"
                    dataSource="intervalDaysDatasource" required="true">
                    <caption>Start Date</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
                <property key="intervalDaysEnd" type="attribute"
                    dataSource="intervalDaysDatasource" required="true">
                    <caption>End Date</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
            </propertyGroup>
            <propertyGroup caption="Disable days of the week">
                <property key="disableSunday" type="expression" required="true" defaultValue="false">
                    <caption>Disable Sunday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableMonday" type="expression" required="true" defaultValue="false">
                    <caption>Disable Monday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableTuesday" type="expression" required="true"
                    defaultValue="false">
                    <caption>Disable Tuesday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableWednesday" type="expression" required="true"
                    defaultValue="false">
                    <caption>Disable Wednesday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableThursday" type="expression" required="true"
                    defaultValue="false">
                    <caption>Disable Thursday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableFriday" type="expression" required="true" defaultValue="false">
                    <caption>Disable Friday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="disableSaturday" type="expression" required="true"
                    defaultValue="false">
                    <caption>Disable Saturday?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Selectable Times">
            <propertyGroup caption="Min and Max">
                <property key="minTime" type="expression" required="false">
                    <caption>Min time</caption>
                    <description></description>
                    <returnType type="DateTime" />
                </property>
                <property key="maxTime" type="expression" required="false">
                    <caption>Max time</caption>
                    <description></description>
                    <returnType type="DateTime" />
                </property>
            </propertyGroup>
            <propertyGroup caption="Specific times">
                <property key="specificTimesMode" type="enumeration" defaultValue="OFF">
                    <caption>Mode</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="OFF">Off</enumerationValue>
                        <enumerationValue key="INCLUDE">Include</enumerationValue>
                        <enumerationValue key="EXCLUDE">Exclude</enumerationValue>
                    </enumerationValues>
                </property>
                <property key="specificTimesDatasource" type="datasource" isList="true"
                    required="true">
                    <caption>Datasource</caption>
                    <description></description>
                </property>
                <property key="specificTimeAttribute" type="attribute"
                    dataSource="specificTimesDatasource" required="true">
                    <caption>Attribute</caption>
                    <description></description>
                    <attributeTypes>
                        <attributeType name="DateTime" />
                    </attributeTypes>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Customization">
            <propertyGroup caption="Functionality">
                <property key="clearable" type="expression" required="true"
                    defaultValue="true">
                    <caption>Clearable?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="openToDate" type="expression" required="false"
                    defaultValue="">
                    <caption>Open to date</caption>
                    <description></description>
                    <returnType type="DateTime" />
                </property>
                <property key="maskInput" type="boolean" defaultValue="false">
                    <caption>Mask input?</caption>
                    <description></description>
                </property>
                <property key="useCustomContent" type="boolean" defaultValue="false">
                    <caption>Use custom content?</caption>
                    <description></description>
                </property>
                <property key="customContent" type="widgets" required="true">
                    <caption>Custom content</caption>
                    <description></description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Icon">
                <property key="showIcon" type="boolean" defaultValue="true">
                    <caption>Show icon?</caption>
                    <description></description>
                </property>
                <property key="showIconInside" type="boolean" defaultValue="false">
                    <caption>Show icon inside?</caption>
                    <description></description>
                </property>
                <property key="customIcon" type="icon" required="false">
                    <caption>Custom icon</caption>
                    <description></description>
                </property>
            </propertyGroup>
            <propertyGroup caption="Today button">
                <property key="showTodayButton" type="boolean"
                    defaultValue="true">
                    <caption>Show today button?</caption>
                    <description></description>
                </property>
                <property key="todayButtonText" type="textTemplate" required="false">
                    <caption>Today button text</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Today</translation>
                        <translation lang="nl_NL">Vandaag</translation>
                    </translations>
                </property>
            </propertyGroup>
            <propertyGroup caption="Month display">
                <property key="monthsToDisplay" type="expression" required="true"
                    defaultValue="1">
                    <caption>Months to display</caption>
                    <description></description>
                    <returnType type="Integer" />
                </property>
                <property key="showPreviousMonth" type="expression" required="true"
                    defaultValue="false">
                    <caption>Show previous months?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
            </propertyGroup>
            <propertyGroup caption="Style">
                <property key="showInline" type="expression" required="true"
                    defaultValue="false">
                    <caption>Show inline?</caption>
                    <description></description>
                    <returnType type="Boolean" />
                </property>
                <property key="showWeekNumbers" type="boolean"
                    defaultValue="false">
                    <caption>Show week numbers?</caption>
                    <description></description>
                </property>
                <property key="showArrow" type="boolean" defaultValue="false">
                    <caption>Show arrow?</caption>
                    <description></description>
                </property>
                <property key="alignment" type="enumeration" defaultValue="LEFT">
                    <caption>Alignment</caption>
                    <description></description>
                    <enumerationValues>
                        <enumerationValue key="LEFT">Left</enumerationValue>
                        <enumerationValue key="RIGHT">Right</enumerationValue>
                        <enumerationValue key="AUTO">Auto</enumerationValue>
                    </enumerationValues>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Events">
            <propertyGroup caption="On change">
                <property key="onChange" type="action">
                    <caption>On change</caption>
                    <description></description>
                </property>
            </propertyGroup>
            <propertyGroup caption="On enter">
                <property key="onEnter" type="action">
                    <caption>On enter</caption>
                    <description></description>
                </property>
            </propertyGroup>
            <propertyGroup caption="On leave">
                <property key="onLeave" type="action">
                    <caption>On leave</caption>
                    <description></description>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Accessibility">
            <propertyGroup caption="General">
                <property key="required" type="expression" required="true" defaultValue="false">
                    <caption>Required?</caption>
                    <description> </description>
                    <returnType type="Boolean" />
                </property>
            </propertyGroup>
            <propertyGroup caption="Labels">
                <property key="calendarIconLabel" type="textTemplate" required="true">
                    <caption>Calendar icon label</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Toggle calendar</translation>
                        <translation lang="nl_NL">Wijzig</translation>
                    </translations>
                </property>
                <property key="navigateButtonPrefix" type="textTemplate" required="true">
                    <caption>Navigate button prefix</caption>
                    <description>Used for the header arrows as '{Show} {Year/Month}'</description>
                    <translations>
                        <translation lang="en_US">Show</translation>
                        <translation lang="nl_NL">Toon</translation>
                    </translations>
                </property>
                <property key="selectPrefix" type="textTemplate" required="true">
                    <caption>Select prefix</caption>
                    <description>Used on focus of each day</description>
                    <translations>
                        <translation lang="en_US">Select</translation>
                        <translation lang="nl_NL">Selecteer</translation>
                    </translations>
                </property>
                <property key="weekPrefix" type="textTemplate" required="true">
                    <caption>Week prefix</caption>
                    <description>Used for announcing the week number as '{Week} {WeekNumber}'</description>
                    <translations>
                        <translation lang="en_US">Week</translation>
                        <translation lang="nl_NL">Week</translation>
                    </translations>
                </property>
                <property key="monthPrefix" type="textTemplate" required="true">
                    <caption>Month prefix</caption>
                    <description>Used for announcing the month container</description>
                    <translations>
                        <translation lang="en_US">Month</translation>
                        <translation lang="nl_NL">Maand</translation>
                    </translations>
                </property>
                <property key="monthSelectLabel" type="textTemplate" required="true">
                    <caption>Month select label</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Select Month</translation>
                        <translation lang="nl_NL">Selecteer maand</translation>
                    </translations>
                </property>
                <property key="yearSelectLabel" type="textTemplate" required="true">
                    <caption>Year select label</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Select Year</translation>
                        <translation lang="nl_NL">Selecteer jaar</translation>
                    </translations>
                </property>
                <property key="disabledLabel" type="textTemplate" required="true">
                    <caption>Disabled label</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Not selectable</translation>
                        <translation lang="nl_NL">Niet mogelijk</translation>
                    </translations>
                </property>
                <property key="clearButtonLabel" type="textTemplate" required="true">
                    <caption>Clear button label</caption>
                    <description></description>
                    <translations>
                        <translation lang="en_US">Clear</translation>
                        <translation lang="nl_NL">Annuleren</translation>
                    </translations>
                </property>
            </propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Common">
            <propertyGroup caption="Common">
                <systemProperty key="Name" />
                <systemProperty key="TabIndex" />
            </propertyGroup>
            <propertyGroup caption="Visibility">
                <systemProperty key="Visibility" />
            </propertyGroup>
            <propertyGroup caption="Editbility">
                <systemProperty key="Editability" />
            </propertyGroup>
        </propertyGroup>
    </properties>
</widget>